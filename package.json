  "description": "Single page website with a collection of short stories",
  "main": "./server/index.js",
  "scripts": {
    "dev-server": "nodemon --watch ./server --exec node-env-run -- ./server",
    "webpack": "webpack --color",
    "eslit": "eslint ./src ./server --color",
    "mocha": "mocha --colors ./test/**/*.js",
    "postcss": "postcss ./public/*.css --replace",
    "prod": "cross-env NODE_ENV=production webpack --color --progress",
    "build": "npm run prod && npm run postcss",
    "test": "npm run eslit && npm run mocha",
    "start": "node ./server"
